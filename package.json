  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "codecov": "^3.0.2",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
